{
  "name": "cdk8s-eval",
  "version": "1.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "rminst": "rm -rf node_modules && npm install",
    "lint": "eslint .",
    "prettier": "prettier --write .",
    "import": "cdk8s import",
    "synth": "cdk8s synth",
    "compile": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "build": "npm run compile && npm run test && npm run synth",
    "upgrade": "npm i cdk8s@latest cdk8s-cli@latest",
    "upgrade:next": "npm i cdk8s@next cdk8s-cli@next",
    "compile-synth": "./scripts/compile-synth.sh",
    "minikube-test": "./scripts/minikube-test.sh",
    "precommit": "./scripts/pre-commit-hook.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./scripts/pre-commit-hook.sh && git stage ."
    }
  },
  "dependencies": {
    "cdk8s": "^0.19.0",
    "constructs": "^2.0.1",
    "dotenv": "^8.2.0",
    "eslint-config-with-prettier": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.5",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "babel-jest": "^25.5.1",
    "cdk8s-cli": "^0.19.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "prettier": "^2.0.5",
    "prettier-eslint": "^9.0.1",
    "ts-jest": "^25.5.1",
    "typescript": "^3.8.3"
  }
}
